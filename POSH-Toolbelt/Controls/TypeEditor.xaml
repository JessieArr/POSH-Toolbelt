<services:FileEditor x:Class="POSH_Toolbelt.Controls.TypeEditor"
             xmlns:services="clr-namespace:POSH_Toolbelt.Services"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:POSH_Toolbelt.Controls"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="25"></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition Height="25"></RowDefinition>
        </Grid.RowDefinitions>
        <Label Grid.Row="0">Place each available list item on a line by itself.</Label>
        <TextBox Grid.Row="1" Name="ListItems" TextChanged="ListItems_TextChanged" AcceptsReturn="True" ></TextBox>        
    </Grid>
</services:FileEditor>
